{
    "name": "maiorano84/object-hydrator",
    "description": "Hydration package for PHP Objects",
    "type": "library",
    "license": "MIT",
    "keywords": ["hydration", "entities"],
    "autoload": {
        "psr-4": {
            "Maiorano\\ObjectHydrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maiorano\\ObjectHydrator\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Matt Maiorano",
            "email": "maiorano84@gmail.com"
        }
    ],
    "require": {
        "php": ">= 8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "phpunit": "phpunit --verbose --coverage-clover=clover.xml"
    }
}
